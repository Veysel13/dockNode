{
  "name": "sequalizeimport",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon index.ts",
    "queue:work": "ts-node src/lib/rabbitmq/consumer.ts",
    "start": "npx ts-node index.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "make:migrate": "node -e \"require('child_process').execSync('npx sequelize-cli migration:generate --name ' + process.argv[1])\"",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "make:model": "node -e \"require('child_process').execSync('npx sequelize-cli model:generate --name ' + process.argv[1] + ' --attributes name:string', { stdio: 'inherit' })\"",
    "make:seed": "node -e \"require('child_process').execSync('npx sequelize-cli seed:generate --name ' + process.argv[1])\"",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "testOld": "echo \"Error: no test specified\" && exit 1",
    "test": "jest --detectOpenHandles"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "amqplib": "^0.10.5",
    "body-parser": "^1.20.3",
    "bullmq": "^5.49.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dataloader": "^2.2.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-graphql": "^0.12.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "fs": "^0.0.1-security",
    "graphql": "^16.10.0",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
    "i18n": "^0.15.1",
    "ioredis": "^5.6.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "memjs": "^1.3.2",
    "memory-cache": "^0.2.0",
    "node-cron": "^3.0.3",
    "path": "^0.12.7",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "request-ip": "^3.3.0",
    "sequelize": "^6.37.5",
    "util": "^0.12.5",
    "valid-url": "^1.0.9",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.6",
    "@types/express": "^5.0.0",
    "@types/i18n": "^0.13.12",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/memjs": "^1.3.3",
    "@types/memory-cache": "^0.2.6",
    "@types/node": "^22.13.1",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
